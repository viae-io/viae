{
  "name": "viae",
  "version": "2.0.0-beta.1",
  "private": false,
  "description": "bi-directional binary streaming req/res framework",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {  
    "clean": "cross-env rimraf .tmp .nyc_output coverage dist",
    "build": "tsc --build tsconfig.build.json",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viae-io/viae.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "api",
    "middleware",
    "request",
    "response",
    "websocket",
    "async-iterable",
    "rxjs",
    "rowan",
    "ws"
  ],
  "author": "Meirion Hughes <crakinshot@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viae-io/viae/issues"
  },
  "homepage": "https://github.com/viae-io/viae#readme",
  "devDependencies": {
    "@types/ws": "latest",
    "concurrently": "latest",
    "cross-env": "latest",
    "rimraf": "latest",
    "typescript": "latest",
    "web-streams-extensions": "latest",
    "web-streams-polyfill": "latest",
    "ws": "latest"
  },
  "dependencies": {
    "@viae/core": "^2.0.0-beta.1",
    "@viae/pb": "^2.0.0-beta.1",
    "cbor-x": "^1.5.9",
    "msgpackr": "^1.10.2",
    "path-to-regexp": "^6",
    "reflect-metadata": "~0.2.2",
    "rowan": "^2.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules/",
      "dist/"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "gitHead": "6e99bb759a0211ec8f1bff306a7a169b016b10ff"
}
